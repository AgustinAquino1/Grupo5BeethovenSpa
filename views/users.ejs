<!DOCTYPE html>
<html lang="en">

<%- include ('partials/head')%>

<body>

    <%- include ('partials/header')%>


        <main>


                <section class = "servicios-main">

                    <div class = "perfiles-container">

                <% users.forEach(user => { %>
                    
                    <article class="card-users">
                        <a href="/users/profile/<%= user.id %> ">
                            <img class="imagenes-perfiles" src="/img/users/<%= user.image %>">
                            <div class="perfil-nombre">
                                <i class="fa-solid fa-paw"></i>
                                <h3 class="nombre-prefiles"><%= user.name %></h3>
                            </div>
                        </a>
                    </article>
                <%}) %>

                    </div>
                </section>



            </div>
        </main>

        <%- include ('partials/footer')%>

</body>
</html>